{
	"css.reset.default": {
		"scope": "css,scss,sass,less",
		"prefix": "rdf",
		"body": [
			"*, *::before, *::after {",
			"    margin: 0;",
			"    padding: 0;",
			"    box-sizing: border-box;",
			"}",
			"",
			":root {",
			"    line-height: 1.5;",
			"    font-weight: 400;",
			"    font-family: Inter, system-ui, Avenir, Helvetica, Arial, sans-serif;",
			"    font-synthesis: none;",
			"",
			"    text-wrap: balance;",
			"    text-rendering: optimizeLegibility;",
			"    -webkit-font-smoothing: antialiased;",
			"    -moz-osx-font-smoothing: grayscale;",
			"}",
			"",
			"body {",
			"    min-height: 100dvh;",
			"",
			"    display: flex;",
			"    flex-direction: column;",
			"    padding: 5vh clamp(1rem,5vw,3rem) 1rem;",
			"",
			"    color: #67697b;",
			"}"
		]
	},
	"css.reset.image": {
		"scope": "css,scss,sass,less",
		"prefix": "rim",
		"body": [
			"img {",
			"    display: block;",
			"",
			"    object-fit: cover;",
			"    object-position: center;",
			"",
			"    max-block-size: 100%;",
			"    max-inline-size: 100%;",
			"}"
		]
	},
	"css.reset.anchor": {
		"scope": "css,scss,sass,less",
		"prefix": "ran",
		"body": [
			"a {",
			"    color: inherit;",
			"    text-decoration: none;",
			"}"
		]
	},
	"css.reset.button": {
		"scope": "css,scss,sass,less",
		"prefix": "rbt",
		"body": [
			"button {",
			"    display: block;",
			"",
			"    cursor: point;",
			"    background-color: transparent;",
			"    border: 1px solid #e5e7eb;",
			"    border-radius: 0.5rem;",
			"}"
		]
	},
	"css.reset.list": {
		"scope": "css,scss,sass,less",
		"prefix": "rli",
		"body": [
			"li {",
			"    list-style: none;",
			"}"
		]
	}
}